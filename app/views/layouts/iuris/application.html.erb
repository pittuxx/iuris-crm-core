<!DOCTYPE html>
<html>
<head>
  <title>Core</title>
  <%= stylesheet_link_tag    "iuris/application", media: "all", 'data-turbolinks-track' => true %>
  <%= javascript_include_tag "iuris/application", 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'layouts/iuris/nav' %>

<div class="container" role="main">
	<% flash.each do |key, value| %>
		<div class="<%= flash_class(key.to_sym) %>"><%= value %></div>
	<% end %>
	<div class="jumbotron">
		<%= yield %>
	</div>
</div>

</body>
</html>
